              <div class="col"><a href="https://vallesmd.com/personal/med/acls/index.html" class="btn btn-secondary btn-lg active" tabindex="-1" role="button" aria-disabled="true">ACLS</a></div>
              <div class="col"><a href="https://vallesmd.com/personal/med/glasgow/index.html" class="btn btn-secondary btn-lg active" tabindex="-1" role="button" aria-disabled="true">Glasgow</a></div>
            </div>
          </div>
      </div>

      <div id="clima">
        <br><br><br>
        <h4>Ofrecemos nuestro servicio de Clima</h4>
        <br>
        <form>
        <input placeholder="Location">
        <button class="btn btn-secondary btn-lg active">Seach</button>
        </form>
      </div>
      <br>
        <p style="color: darkslategray" id="message-1"></p>
        <P id="message-2"></P>
    </div>
    <div id="columna-derecha" class="col"></div>
    </div>
    </div>
  </div>
    <div class="container">
      <div class="row">
        <div class="col-4"></div>
        <div class="col-7">
     {{>footer}}
        </div>
